<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DogWalkers.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DogWalkers.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Dog Walker's Pocket App">

    <TabBar>
        <ShellContent Title="My Dogs List" Route="DogsListPage" ContentTemplate="{DataTemplate local:DogsListPage}" />
        <ShellContent Title="Other Dogs List" Route="OtherDogsPage" ContentTemplate="{DataTemplate local:OtherDogsPage}" />
    </TabBar>

</Shell>

